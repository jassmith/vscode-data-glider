{
  "name": "vscode-data-glider",
  "displayName": "Glide Data Grid Renderer",
  "description": "Glide Data Grid Renderer for VScode Notebooks",
  "version": "1.0.0",
  "engines": {
    "node": ">14.16.0",
    "vscode": "^1.62.0"
  },
  "icon": "resources/icons/data-glider.png",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak",
  "contributors": [
    "Taras Novak"
  ],
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/vscode-data-glider"
  },
  "bugs": "https://github.com/RandomFractals/vscode-data-glider/issues",
  "homepage": "https://github.com/RandomFractals/vscode-data-glider/README.md",
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "categories": [
    "Data Science",
    "Formatters",
    "Education",
    "Machine Learning",
    "Notebooks",
    "Visualization"
  ],
  "keywords": [
    "glide",
    "data grid",
    "notebookRenderer",
    "JSON",
    "CSV",
    "flat data",
    "text data",
    "view",
    "notebook",
    "cell",
    "output",
    "renderer"
  ],
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "data-glider",
        "entrypoint": "./out/renderer/index.js",
        "displayName": "Data Glider",
        "mimeTypes": [
          "application/json",
          "text/csv"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/d3-dsv": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/vscode": "^1.62.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vscode-notebook-error-overlay": "^1.0.1",
    "@vscode/test-electron": "^1.6.2",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "d3-dsv": "^3.0.1"
  }
}
